<?xml version="1.0" encoding="utf-8"?>
<network-security-config xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <domain-config>
        <domain includeSubdomains="true">aws.covidsafe.gov.au
        </domain>
        <trust-anchors>
            <certificates src="@raw/amazon_root_ca_1"/>
            <certificates src="@raw/amazon_root_ca_2"/>
            <certificates src="@raw/amazon_root_ca_3"/>
            <certificates src="@raw/amazon_root_ca_4"/>
